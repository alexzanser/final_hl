{
    "id": null,
    "title": "Go Service Metrics",
    "tags": [
        "go-service",
        "prometheus"
    ],
    "timezone": "browser",
    "panels": [
        {
            "id": 1,
            "title": "RPS",
            "type": "timeseries",
            "targets": [
                {
                    "expr": "sum(rate(service_rps_total[5m]))",
                    "legendFormat": "RPS"
                }
            ],
            "datasource": "Prometheus",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 0
            },
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "palette-classic"
                    }
                },
                "overrides": []
            },
            "options": {
                "legend": {
                    "showLegend": true
                },
                "tooltip": {
                    "mode": "single"
                }
            }
        },
        {
            "id": 2,
            "title": "Latency (p95)",
            "type": "timeseries",
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, sum(rate(service_handle_latency_seconds_bucket[5m])) by (le))",
                    "legendFormat": "p95 latency"
                }
            ],
            "datasource": "Prometheus",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 0
            },
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "palette-classic"
                    }
                },
                "overrides": []
            },
            "options": {
                "legend": {
                    "showLegend": true
                },
                "tooltip": {
                    "mode": "single"
                }
            }
        },
        {
            "id": 3,
            "title": "Anomaly Rate",
            "type": "timeseries",
            "targets": [
                {
                    "expr": "sum(rate(service_anomalies_total[5m]))",
                    "legendFormat": "Anomalies/min"
                }
            ],
            "datasource": "Prometheus",
            "gridPos": {
                "h": 8,
                "w": 24,
                "x": 0,
                "y": 8
            },
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "palette-classic"
                    }
                },
                "overrides": []
            },
            "options": {
                "legend": {
                    "showLegend": true
                },
                "tooltip": {
                    "mode": "single"
                }
            }
        }
    ],
    "time": {
        "from": "now-15m",
        "to": "now"
    },
    "timepicker": {},
    "templating": {
        "list": []
    },
    "annotations": {
        "list": []
    },
    "refresh": "10s",
    "schemaVersion": 38,
    "version": 1,
    "links": []
}